{
	"name": "PracticeTest",
	"properties": {
		"folder": {
			"name": "Test"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "girishdl_ls",
						"type": "LinkedServiceReference"
					},
					"name": "udataParquet"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          movie_id as string,",
				"          cust_id as string,",
				"          rating as string,",
				"          release_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet',",
				"     fileSystem: 'girishdata',",
				"     folderPath: 'files/rawdata/parquet_files',",
				"     compressionCodec: 'none') ~> udataParquet"
			]
		}
	}
}
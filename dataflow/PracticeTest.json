{
	"name": "PracticeTest",
	"properties": {
		"folder": {
			"name": "Test"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "udataParquet"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie_id as string,",
				"          cust_id as string,",
				"          rating as string,",
				"          release_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     fileSystem: 'girishdata',",
				"     folderPath: 'files/rawdata/parquet_files',",
				"     compressionCodec: 'none') ~> udataParquet",
				"udataParquet keyGenerate(output(SK as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'udataSkParquet',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'parquet',",
				"     fileSystem: 'girishdata',",
				"     folderPath: 'files/rawdata/parquet_files',",
				"     compressionCodec: 'none',",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1"
			]
		}
	}
}
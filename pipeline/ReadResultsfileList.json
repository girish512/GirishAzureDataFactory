{
	"name": "ReadResultsfileList",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_resultsfile",
						"type": "DatasetReference",
						"parameters": {
							"FolderDate": {
								"value": "@pipeline().parameters.FolderDate",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"columnCount"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "JsonReadSettings"
					}
				}
			}
		],
		"parameters": {
			"FolderDate": {
				"type": "string",
				"defaultValue": "2021-03-21"
			},
			"FileSystem": {
				"type": "string",
				"defaultValue": "data"
			},
			"FilePath": {
				"type": "string",
				"defaultValue": "Files/Input/f1db_csv"
			},
			"FileName": {
				"type": "string",
				"defaultValue": "results.json"
			}
		},
		"variables": {
			"filetype": {
				"type": "String"
			}
		},
		"annotations": []
	}
}